		{
			"bold": true,
			"color": "#ff5555",
			"italic": true,
			"tag": "warning"
		},
		{
			"bold": true,
			"color": "#ff79c6",
			"italic": true,
			"tag": "??"
		},
		{
			"bold": true,
			"color": "#ff79c6",
			"italic": true,
			"tag": "question"
		},
		{
			"bold": true,
			"color": "#8be9fd",
			"italic": true,
			"tag": "info"
		},
		{
			"color": "#6272a4",
			"italic": true,
			"strikethrough": true,
			"tag": "//"
		},
		{
			"bold": true,
			"color": "#ffb86c",
			"italic": true,
			"tag": "fixme"
		},
		{
			"bold": true,
			"color": "#f8f8f2",
			"italic": true,
			"tag": "‚îÅ"
		},
		{
			"bold": true,
			"color": "#f8f8f2",
			"italic": true,
			"tag": "‚îÄ"
		},
		{
			"color": "#f8f8f2",
			"italic": true,
			"tag": "##",
			"underline": true
		},
		{
			"bold": true,
			"color": "#50fa7b",
			"italic": true,
			"tag": "%%"
		},
		{
			"bold": true,
			"color": "#50fa7b",
			"italic": true,
			"tag": ">>"
		}
	],
